<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<nodedefs>   
<nodedef name="ZWaveSource" class="vdab.extnodes.zwave.ZWaveSource">
  	<attr name="ConsumedEventTypes">NONE</attr>
    <attr name="PublishedEventTypes">String,Event</attr>
    <attr name="NodeDescTemplate"> ${ZWavePort}</attr>
    <attr name="NodeOverview">Prototype which reads captures images from a Foscam Security Camera using appropriate HTTP posts. The address and port must be accessible and specified.</attr>
    <attr name="NodeQuality" >Prototype</attr>   
	<datadef name="ZWavePort">
		<attr name="EditOrder" type="Integer">2</attr>
		<attr name="Category">Required</attr>
		<attr name="Desc">ZWave device port</attr>				
	</datadef>
 </nodedef>
 <nodedef name="ZWaveTarget" class="vdab.extnodes.zwave.ZWaveTarget">
  	<attr name="ConsumedEventTypes">Trigger,String,Event</attr>
    <attr name="PublishedEventTypes">NONE</attr>
    <attr name="NodeDescTemplate"> ${ZWavePort}</attr>
    <attr name="NodeOverview">Prototype which reads captures images from a Foscam Security Camera using appropriate HTTP posts. The address and port must be accessible and specified.</attr>
    <attr name="NodeQuality" >Prototype</attr>   
	<datadef name="ZWavePort">
		<attr name="EditOrder" type="Integer">2</attr>
		<attr name="Category">Required</attr>
		<attr name="Desc">ZWave device port</attr>				
	</datadef>
	<datadef name="DeviceType">
		<attr name="EditOrder" type="Integer">3</attr>
		<attr name="Category">Required</attr>
   		<attr name="RefreshOnChange" type="Boolean">true</attr>
		<attr name="Enum">ZWaveDeviceType</attr>
		<attr name="Desc">Pick the proper ZWave device type.</attr>				
	</datadef>
	<datadef name="NodeName">
		<attr name="EditOrder" type="Integer">4</attr>
		<attr name="Category">Required</attr>
		<attr name="Desc">Pick from the existing nodes of this type.</attr>				
	</datadef>
 </nodedef>
</nodedefs>